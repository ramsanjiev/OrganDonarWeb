<%- include  ('partials/header.ejs') -%>

<div class="container">
    <table class="table table-hover table-dark" id="tab1">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">City</th>
                <th scope="col">Conatct No</th>
            </tr>
        </thead>
        <tbody>
            <% dbs.forEach(function(dbs) {  %>
            <tr>
                <th><%= dbs.FirstName %></th>
                <th><%= dbs.City %></th>
                <th><%= dbs.Contact %></th>
                <th><a href="/donor/edit/<%= dbs._id %> "><i class="fas fa-user-edit" id="repot"></i></a></th>
                <th>
                    <form action='/donor/edit/<%= dbs._id %>?_method=DELETE' method="POST" id="delete">
                        <button class='btn'><a><i class="fas fa-exclamation-triangle" id="repot"></i></a></button>
                    </form>
                </th>
            </tr>
            <% }); %>
        </tbody>
    </table>
</div>




<%- include ('partials/footer.ejs')-%>